{
  "expo": {
    "name": "expo-native-language-picker-example",
    "slug": "expo-native-language-picker-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.dylmye.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.dylmye.example"
    },
    "plugins": [
      [
        "../app.plugin.js",
        {
          "languages": ["en_GB", "fr_FR"]
        }
      ]
    ]
  }
}
